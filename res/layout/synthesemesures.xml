<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/background"
    >
    
<TextView
	android:id="@+id/synthese_titre"
	android:layout_width="wrap_content" 
    android:layout_height="wrap_content" 
    android:text="@string/synthese_titre"
    android:layout_gravity="center"
    android:textSize="25px"
    android:layout_marginTop="10px"
    android:textColor="#0000FF"
    android:layout_centerHorizontal="true">
   
</TextView>

	<Button
 		android:id="@+id/synthese_retour"
		android:layout_width="wrap_content"
 		android:layout_height="wrap_content"
 		android:layout_gravity="center"
 		android:layout_below="@id/synthese_titre"
 		android:text="@string/synthese_retour"
		android:layout_centerHorizontal="true"
		android:background="@drawable/mybutton2"
 	/>

<TextView
	android:id="@+id/synthese_reptexte"
	android:layout_width="wrap_content" 
    android:layout_height="wrap_content" 
    android:text="@string/synthese_reptexte"
    android:layout_below="@id/synthese_retour"
    android:layout_gravity="left"
    android:layout_marginLeft="10px"
    android:textSize="15px"
    android:layout_marginTop="20px"
    android:textColor="#000000"/>
    
    
    <TextView
    android:id="@+id/synthese_rep"
	android:layout_width="wrap_content" 
    android:layout_height="wrap_content" 
    android:text="/"
    android:layout_below="@id/synthese_reptexte"
    android:layout_gravity="left"
    android:layout_marginLeft="10px"
    android:textSize="15px"
    android:textColor="#000000"/>
    
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="fill_parent"
    android:stretchColumns="5"
    android:layout_marginTop="40px"
    android:id="@+id/synthese_tableau">

    <TableRow android:id="@+id/synthese_tableau_row0">
        <TextView
            android:layout_column="1"
            android:text="Photo"  
        	android:textColor="#ED0000"
        	android:paddingLeft="10px"
        	
        />
        <TextView
        	android:layout_column="2"
            android:text="Date"
            android:textColor="#ED0000"
            android:paddingLeft="10px"
          />
        <TextView
            android:layout_column="3"
            android:text="Heure"
            android:textColor="#ED0000"
            android:paddingLeft="10px"
          />
        <TextView
        	android:layout_column="4"
            android:text="Etat"
            android:textColor="#ED0000"
            android:paddingLeft="10px"
          />
        <TextView
        	android:layout_column="5"
            android:text="PrÃ©cision"
            android:textColor="#ED0000"
            android:paddingLeft="10px"
          />
    </TableRow>
    <View
        android:layout_height="2dip"
        android:background="#FF909090" />
       
            
     <TableRow
     android:id="@+id/synthese_tableau_row1">
        <Button
            android:id="@+id/synthese_l1c1"
            android:layout_column="1"
            android:textColor="@color/couleurTableau"
            android:paddingLeft="10px"
            android:background="@null"
            />
            
  	    <TextView
            android:id="@+id/synthese_l1c2"
            android:layout_column="2"
            android:textColor="@color/couleurTableau"
            android:paddingLeft="10px"
            />
            
        <TextView
            android:id="@+id/synthese_l1c3"
            android:layout_column="3"
            android:textColor="@color/couleurTableau"
            android:paddingLeft="10px"
            />      
            
        <Spinner
            android:id="@+id/synthese_l1c4"
            android:layout_width="fill_parent"
			android:layout_height="wrap_content"
            android:layout_column="4"
            android:paddingLeft="10px"
            android:background="@null"
            android:textColor="@color/couleurTableau"
            />
		<TextView
            android:id="@+id/synthese_l1c5"
            android:layout_column="5"
            android:textColor="@color/couleurTableau"
            android:paddingLeft="10px"
            />        
	</TableRow>
	<View
       android:layout_height="2dip"
       android:background="#FF909090"/>
           
           
           
     <TableRow android:id="@+id/synthese_tableau_row2">
        <Button
            android:id="@+id/synthese_l2c1"
            android:layout_column="1"
            android:textColor="@color/couleurTableau"
            android:paddingLeft="10px"
            android:background="@null" />
            
  	    <TextView
            android:id="@+id/synthese_l2c2"
            android:layout_column="2"
            android:textColor="@color/couleurTableau"
            android:paddingLeft="10px" />
            
        <TextView
            android:id="@+id/synthese_l2c3"
            android:layout_column="3"
            android:textColor="@color/couleurTableau"
            android:paddingLeft="10px" />      
            
        <Spinner
            android:id="@+id/synthese_l2c4"
            android:layout_width="fill_parent"
			android:layout_height="wrap_content"
            android:layout_column="4"
            android:paddingLeft="10px"
            android:background="@null"
            android:textColor="@color/couleurTableau" /> 
         <TextView
            android:id="@+id/synthese_l2c5"
            android:layout_column="5"
            android:textColor="@color/couleurTableau"
            android:paddingLeft="10px" />       
	</TableRow>
	<View
       android:layout_height="2dip"
       android:background="#FF909090" />

</TableLayout>



</LinearLayout>
